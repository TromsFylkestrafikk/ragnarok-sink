{
    "name": "tromsfylkestrafikk/ragnarok-sink",
    "description": "Sink API for Ragnarok",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "keywords": [
		"library"
	],
    "authors": [
        {
            "name": "KÃ¥re Slettnes",
            "email": "kaare.slettnes@tffk.no"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "illuminate/console": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/support": "^10.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.6",
        "phpmd/phpmd": "@stable",
        "phpstan/phpstan": "^1.8",
        "nunomaduro/larastan": "^1.0|^2.0",
        "orchestra/testbench": "^6.23|^7.6",
        "orchestra/canvas": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "Tromsfylkestrafikk\\RagnarokSink\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tromsfylkestrafikk\\RagnarokSink\\Tests\\":"tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tromsfylkestrafikk\\RagnarokSink\\RagnarokSinkServiceProvider"
            ],
            "aliases": {
                "Tromsfylkestrafikk": "Tromsfylkestrafikk\\RagnarokSink\\RagnarokSinkFacade"
            }
        }
    }
}
